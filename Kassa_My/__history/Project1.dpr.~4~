program Project1;

uses
  Forms,
  Unit1 in 'Unit1.pas' {Form1},
  rfUnit in 'rfUnit.pas',
  rfReq in 'rfReq.pas';

{$R *.res}

begin
  Application.Initialize;
  try
      Application.CreateForm(TForm1, Form1);
      Application.Run;
  except
  on E: Exception do
  begin
       Application.MessageBox(E.Message, 'Фатальная ошибка');
       Halt;
  end;
  end;
end.
